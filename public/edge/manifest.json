{
  "manifest_version": 2,
  "name": "Jira Assistant: Worklog, Sprint report, etc",
  "short_name": "Jira Assist",
  "version": "2.37",
  "description": "Help to track & upload work done on a Jira Ticket on timely basis and generate Worklog, Sprint and other custom reports.",
  "icons": {
    "24": "assets/icon_24.png",
    "32": "assets/icon_32.png",
    "48": "assets/icon_48.png"
  },
  "browser_action": {
    "default_icon": {
      "24": "assets/icon_24.png",
      "32": "assets/icon_32.png",
      "48": "assets/icon_48.png"
    },
    "default_title": "Jira Assistant",
    "default_popup": "/menu.html"
  },
  "background": {
    "persistent": true,
    "page": "/background.html"
  },
  "commands": {
    "open-ja-dashboard": {
      "suggested_key": {
        "default": "Alt+J",
        "mac": "Command+Shift+J"
      },
      "description": "Open JiraAssist dashboard page"
    }
  },
  "oauth2": {
    "client_id": "692513716183-s97kv5slq5ihm410jq2kc4r8hr77rcku.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com https://calendar.google.com https://ssl.google-analytics.com https://www.google-analytics.com https://login.microsoftonline.com https://graph.microsoft.com ; object-src 'self';",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "identity",
    "http://*/",
    "https://*/",
    "storage",
    "background",
    "notifications",
    "http://calendar.google.com/calendar/*",
    "https://calendar.google.com/calendar/*",
    "https://login.microsoftonline.com/common/oauth2/v2.0/*",
    "https://graph.microsoft.com/v1.0/me/*"
  ],
  "optional_permissions": [
    "activeTab",
    "unlimitedStorage"
  ],
  "author": "Shridhar"
}